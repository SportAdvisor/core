msgid "Email address is already registered"
msgstr "Пользователь с таким email уже зарегистрирован"

msgid "Email is invalid"
msgstr "Указан неверный адрес электронной почты"

msgid "Field is required"
msgstr "Поле, обязательное для заполнения"

msgid "Your password must be at least 8 characters long, and include at least one lowercase letter, one uppercase letter, and a number. Your password can't contain spaces"
msgstr "Пароль должен быть не короче 8 символов и содержать только латинские символы, маленькую букву, большу букву, цифру. Пароль не должен содержать символ пробела"

msgid "You must accept the end-user license agreement"
msgstr "Вы должны согласиться с пользовательским соглашением"

msgid "Selected language not supported"
msgstr "Выбранный язык не поддерживается"

msgid "Authorization error. Re-login please"
msgstr "Ошибка авторизации. Выполните повторную авторизацию пожалуйста"

msgid "Incorrect password"
msgstr "Неверный пароль"

msgid "Your password reset link has expired. Please initiate a new password reset"
msgstr "Ссылка для сброса пароля не действительна. Пожалуйста, повторите попытку сброса"

msgid "There must be at least %s sport selected but not more than %s"
msgstr "Необходимо выбрать как минимум %s, но не более %s видов спорта"

msgid "At least 1 contact must be defined"
msgstr "Необходимо указать как минимуму 1 контакт"

msgid "Field is invalid"
msgstr "Неверное значение"

msgid "Min value is %s"
msgstr "Минимальное значение %s"

msgid "Max value is %s"
msgstr "Максимальное значение %s"

msgid "Page with sport '%s' already registered (%s)"
msgstr "Страница с дисциплиной '%s' уже зарегистрирована (%s)"

msgid "Exceeded the limit of trainers pages"
msgstr "Превышин лимит страниц тренера"

msgid "Not unique alias"
msgstr "Алиас не уникален"